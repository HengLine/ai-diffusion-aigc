{
  "comfyui": {
    "path": "E:\\Projects\\AI\\ComfyUI",
    "api_url": "http://127.0.0.1:8188",
    "auto_start_server": true
  },
  "models": {
    "stable_diffusion": {
      "path": "",
      "default_model": "v1-5-pruned-emaonly.safetensors",
      "vae": "vae-ft-mse-840000-ema-pruned.safetensors"
    },
    "stable_video_diffusion": {
      "path": "",
      "default_model": "svd.safetensors"
    },
    "flux": {
      "path": "",
      "default_model": "flux1-schnell.safetensors"
    },
    "controlnet": {
      "path": "",
      "models": [
        "control_v11p_sd15_canny.pth",
        "control_v11p_sd15_openpose.pth"
      ]
    },
    "loras": {
      "path": "",
      "presets": {
        "ancient_clothing": [
          "ancient_clothing_lora.safetensors"
        ],
        "sci_fi": [
          "sci_fi_lora.safetensors"
        ]
      }
    }
  },
  "output": {
    "dir": "outputs",
    "format": {
      "image": "png",
      "video": "mp4"
    },
    "quality": {
      "image": 95,
      "video": 25
    }
  },
  "settings": {
    "text_to_image": {
      "width": 512,
      "height": 512,
      "steps": 5,
      "cfg": 3.0,
      "sampler": "DPM++ 2M Karras"
    },
    "image_to_image": {
      "width": 512,
      "height": 512,
      "steps": 5,
      "cfg": 3.0,
      "denoising_strength": 0.75,
      "sampler": "DPM++ 2M Karras"
    },
    "image_to_video": {
      "width": 512,
      "height": 512,
      "frames": 16,
      "fps": 8,
      "motion_bucket_id": 127,
      "noise_aug_strength": 0.02
    },
    "ancient_clothing": {
      "default_prompt": "beautiful woman in traditional Chinese clothing, detailed face, elegant posture, ancient Chinese background",
      "negative_prompt": "modern, low quality, blurry, bad anatomy, disfigured, poor details"
    },
    "sci_fi_video": {
      "default_prompt": "futuristic cityscape, flying cars, neon lights, detailed environment, cinematic lighting",
      "negative_prompt": "low quality, blurry, unrealistic, static, boring"
    }
  }
}